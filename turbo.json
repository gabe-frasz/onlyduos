{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["NEXT_PUBLIC_CLIENT_ID", "NEXT_PUBLIC_CLIENT_SECRET"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
